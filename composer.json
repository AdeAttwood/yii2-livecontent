    "keywords": ["yii2", "edit", "content"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sergey Kozin",
            "email": "sergagame1@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.4",
